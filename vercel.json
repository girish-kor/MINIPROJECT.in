{
  "version": 2,
  "buildCommand": "chmod +x ./build.sh && ./build.sh",
  "outputDirectory": "target",
  "functions": {
    "api/app.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/api/app" }
  ],
  "env": {
    "SPRING_PROFILES_ACTIVE": "vercel"
  }
}
